{
  "expo": {
    "name": "Boundless Movement",
    "slug": "boundless-movement",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "version": "1.0.1",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#000"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "react-native-iap"
    ],
    "ios": {
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "bundleIdentifier": "art.boundless-movement.app",
      "associatedDomains": [
        "applinks:boundless-movement.fitterapp.app",
        "applinks:boundless-movement.mvt.so"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for your profile.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record video to share in your messages.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "so.movement.boundlessmovement",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "fitterApp": "boundless-movement",
      "eas": {
        "projectId": "7c3a5cf2-b16d-4d21-a250-245c3a1c9e86"
      }
    }
  }
}