{
  "name": "fitterapps-unified",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",

    "dev:boundless-movement": "APP_ID=boundless-movement expo start",
    "dev:sorella-online": "APP_ID=sorella-online expo start",
    "dev:sleeptime": "APP_ID=sleeptime expo start",
    "dev:cleer": "APP_ID=cleer expo start",
    "dev:20lifestyle": "APP_ID=20lifestyle expo start",

    "build:boundless-movement:ios": "eas build --profile boundless-movement --platform ios",
    "build:boundless-movement:android": "eas build --profile boundless-movement --platform android",
    "build:sorella-online:ios": "eas build --profile sorella-online --platform ios",
    "build:sorella-online:android": "eas build --profile sorella-online --platform android",
    "build:sleeptime:ios": "eas build --profile sleeptime --platform ios",
    "build:sleeptime:android": "eas build --profile sleeptime --platform android",
    "build:cleer:ios": "eas build --profile cleer --platform ios",
    "build:cleer:android": "eas build --profile cleer --platform android",
    "build:20lifestyle:ios": "eas build --profile 20lifestyle --platform ios",
    "build:20lifestyle:android": "eas build --profile 20lifestyle --platform android"
  },
  "dependencies": {
    "expo": "~52.0.47",
    "expo-device": "~7.0.3",
    "expo-linking": "^7.0.3",
    "expo-notifications": "^0.29.11",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-iap": "^12.15.7",
    "react-native-track-player": "^4.1.1",
    "react-native-webview": "13.12.5",
    "expo-build-properties": "~0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "private": true
}
